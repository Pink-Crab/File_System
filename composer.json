{
    "name": "pinkcrab/wp-file-system",
    "type": "library",
    "description": "Wrapper for WP_File_System that can be standalone or used an implementation of the PHP Leagues FlySystem library.",
    "keywords": [],
    "homepage": "https://pinkcrab.co.uk",
    "license": "MIT",
    "authors": [{
        "name": "Glynn Quelch",
        "email": "glynn.quelch@pinkcrab.co.uk",
        "homepage": "http://clappo.co.uk",
        "role": "Developer"
    }],
    "autoload": {
        "psr-4": {
            "PinkCrab\\Flie_System\\": "src"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "PinkCrab\\Flie_System\\Tests\\": "tests/"
        }
    },
    "repositories": [{
        "url": "https://github.com/Pink-Crab/PHP_Unit_Helpers.git",
        "type": "git"
    }],
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "roots/wordpress": "^5.5",
        "wp-phpunit/wp-phpunit": "^5.0",
        "yoast/phpunit-polyfills": "^0.1.0",
        "symfony/var-dumper": "^5.0",
        "phpstan/phpstan": "^0.12.6",
        "szepeviktor/phpstan-wordpress": "^0.7.2",
        "php-stubs/wordpress-stubs": "^5.6.0",
        "pinkcrab/phpunit-helpers": "dev-master",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
        "wp-coding-standards/wpcs": "^2.1"
    },
    "require": {
        "php": ">=7.1.0"
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "vendor/bin/phpstan analyse src -l8",
        "sniff": "phpcs"
    }
}